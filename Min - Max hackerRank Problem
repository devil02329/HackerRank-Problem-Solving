#include<bits/stdc++.h>
 using namespace std;
 
long long int maxmin(long long int arr[])
    {
        
       long long int mn = LLONG_MAX;
       long long int mx = LLONG_MIN;   
        for(int i=1; i<=5; i++)
            {
               long long int sum = 0;
                for(int j=1; j<=5; j++)
                    {
                        if(i==j)
                            continue;
                        else
                            sum+=arr[j];
                    }
                    
                    if(sum<=mn)  
                        {
                            mn = sum;
                        }
                    if(sum>=mx)
                        {
                            mx = sum;
                        }
                    
                
            }
            
            cout << mn  << " " << mx;
            return 0;
    }
 
int main()
    {   
     long long int arr[5];
        for(int i=1; i<=5; i++)
            {
                cin >> arr[i];
            }
            
            maxmin(arr);
    }
